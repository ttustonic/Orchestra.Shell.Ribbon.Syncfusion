                        <Trigger Property="tools_controls:SplitterAdorner.Orientation" Value="Horizontal">
                            <Setter TargetName="SplitterStackPanel" Property="LayoutTransform">
                                <Setter.Value>
                                    <RotateTransform Angle="-90" />
                                </Setter.Value>
